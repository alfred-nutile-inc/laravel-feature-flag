{
  "name": "alfred-nutile-inc/laravel-feature-flag",
  "description": "Feature Flags for Laravel",
  "authors": [
    {
      "name": "Alfred Nutile Inc",
      "email": "me@alfrednutile.info"
    },
    {
      "name": "Nathan Kirschbaum",
      "email": "nathan@nathankirschbaum.com"
    }
  ],
  "keywords": [
    "feature-flags",
    "feature-toggles",
    "toggles"
  ],
  "require": {
    "php": "~5.6|~7.0",
    "illuminate/support": "*",
    "jowy/feature": "dev-master"
  },
  "require-dev": {
    "codedungeon/phpunit-result-printer": "dev-master",
    "illuminate/support": "<=5.5",
    "mockery/mockery": "0.9.*",
    "orchestra/database": "<=3.5",
    "orchestra/testbench": "<=3.5",
    "phpunit/phpunit": ">=5.4.3",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "AlfredNutileInc\\LaravelFeatureFlags\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}